# Production Environment Configuration for Vercel Deployment
# This file is used when NODE_ENV=production (Vercel deployment)

# External API Configuration
REACT_APP_USE_EXTERNAL_API=true
REACT_APP_EXTERNAL_API_BASE_URL=https://chat.aistocktickers.com
REACT_APP_API_BASE_URL=https://chat.aistocktickers.com
REACT_APP_API_TIMEOUT=30000
REACT_APP_DEBUG_API=false
REACT_APP_DEFAULT_USER_ID=123

# Application Settings
REACT_APP_ENVIRONMENT=production
REACT_APP_DEBUG=false
REACT_APP_ENABLE_LOGGING=false

# Session Management
REACT_APP_DEFAULT_SESSION_ID=0
REACT_APP_MAX_UNRELATED_TOPICS=3
REACT_APP_SESSION_STORAGE_KEY=chatbot_session
REACT_APP_CONVERSATION_STORAGE_KEY=chatbot_conversation

# UI Configuration
REACT_APP_CHATBOT_TITLE=AI Stock Chat
REACT_APP_FLOATING_BUTTON_ENABLED=true
REACT_APP_UI_TYPING_DELAY=1500
REACT_APP_UI_SCROLL_DELAY=300