# External API Configuration Template
# Copy this file to .env.local and update with your API details

# External API Configuration
REACT_APP_USE_EXTERNAL_API=true
REACT_APP_EXTERNAL_API_BASE_URL=https://your-api-domain.com
REACT_APP_DEBUG_API=true

# User Configuration
REACT_APP_DEFAULT_USER_ID=123

# Optional: API Authentication (uncomment and configure if needed)
# REACT_APP_API_TOKEN=your-api-token-here
# REACT_APP_API_KEY=your-api-key-here